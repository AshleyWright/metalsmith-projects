{
  "name": "metalsmith-projects",
  "version": "1.0.1",
  "description": "A Metalsmith plugin for adding metadata for projects (think portfolio content) from GitHub or filesystem (through metalsmith-collections) that can be looped over for creating a portfolio of work.",
  "main": "index.js",
  "scripts": {
    "test": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' mocha --require ts-node/register --watch-extensions ts '**/__tests__/**/*.spec.ts'",
    "build": "pack build"
  },
  "keywords": [
    "metalsmith",
    "projects",
    "portfolio",
    "collection",
    "github"
  ],
  "author": "Ashley Wright",
  "license": "MIT",
  "devDependencies": {
    "@pika/plugin-build-deno": "^0.5.0",
    "@pika/plugin-build-node": "^0.5.1",
    "@pika/plugin-build-types": "^0.5.1",
    "@pika/plugin-standard-pkg": "^0.5.1",
    "@pika/pack": "^0.4.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/jest": "^24.0.6",
    "@types/mocha": "^5.2.6",
    "@types/node": "^13.1.0",
    "@types/ramda": "^0.26.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.0.0",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ]
    ]
  },
  "dependencies": {
    "ramda": "^0.26.1",
    "@octokit/rest": "^17.0.0"
  }
}
